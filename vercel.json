{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "functions": {
    "api/boards/index.ts": {
      "runtime": "@vercel/node"
    },
    "api/boards/[id].ts": {
      "runtime": "@vercel/node"
    },
    "api/boards/[id]/cards.ts": {
      "runtime": "@vercel/node"
    },
    "api/cards/[cardId].ts": {
      "runtime": "@vercel/node"
    },
    "api/cards/[cardId]/move.ts": {
      "runtime": "@vercel/node"
    }
  }
}
